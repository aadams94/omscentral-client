{
  "name": "@omscentral/client",
  "description": "omscentral web client",
  "license": "ISC",
  "author": "Mehmet Bajin <mehmet.bajin@gatech.edu>",
  "homepage": "https://omscentral.com",
  "repository": "github:OMSCentral/omscentral-client",
  "version": "0.0.0",
  "main": "",
  "scripts": {
    "build:local": "env-cmd -f .env.development.local react-scripts build",
    "build:production": "env-cmd -f .env.production react-scripts build",
    "build:staging": "env-cmd -f .env.development react-scripts build",
    "deploy:production": "npm run build:production && firebase deploy --project omscentral-production --only hosting",
    "deploy:staging": "npm run build:staging && firebase deploy --project omscentral-staging --only hosting",
    "format": "run-s format:src format:package",
    "format:package": "prettier-package-json --write ./package.json",
    "format:src": "prettier --write src/",
    "generate": "VERBOSE=true graphql-codegen --config codegen.yml",
    "lint": "eslint src/ --ext .ts,.tsx,.js,.jsx",
    "start": "react-scripts start",
    "test": "react-scripts test"
  },
  "dependencies": {
    "@apollo/react-hooks": "3.1.5",
    "@material-ui/core": "4.10.2",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.56",
    "@sentry/browser": "5.17.0",
    "apollo-cache-inmemory": "1.6.6",
    "apollo-client": "2.6.10",
    "apollo-link": "1.2.14",
    "apollo-link-error": "1.1.13",
    "apollo-link-http": "1.5.17",
    "clsx": "1.1.1",
    "firebase": "7.15.1",
    "graphql": "14.x",
    "graphql-tag": "2.10.3",
    "query-string": "6.13.1",
    "react": "16.13.1",
    "react-apollo": "3.1.5",
    "react-copy-to-clipboard": "5.0.2",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "react-hook-form": "5.7.2",
    "react-markdown": "4.3.1",
    "react-router-dom": "5.2.0",
    "react-visibility-sensor": "5.1.1"
  },
  "devDependencies": {
    "@apollo/react-testing": "3.1.4",
    "@graphql-codegen/cli": "1.15.3",
    "@graphql-codegen/introspection": "1.15.3",
    "@graphql-codegen/typescript": "1.15.3",
    "@graphql-codegen/typescript-operations": "1.15.3",
    "@graphql-codegen/typescript-react-apollo": "1.15.3",
    "@types/firebase": "3.2.1",
    "@types/query-string": "6.3.0",
    "@types/react": "16.9.37",
    "@types/react-copy-to-clipboard": "4.3.0",
    "@types/react-dom": "16.9.8",
    "@types/react-helmet": "6.0.0",
    "@types/react-router-dom": "5.1.5",
    "@typescript-eslint/eslint-plugin": "2.x",
    "@typescript-eslint/parser": "2.x",
    "env-cmd": "10.1.0",
    "eslint": "6.x",
    "eslint-plugin-flowtype": "4.x",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "2.x",
    "eslint-plugin-security": "1.4.0",
    "fibers": "5.0.0",
    "firebase-tools": "8.4.2",
    "node-sass": "4.14.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "prettier-package-json": "2.1.3",
    "react-scripts": "3.4.1",
    "sass": "1.26.8",
    "typescript": "3.9.5"
  },
  "engines": {
    "node": "12.x"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
