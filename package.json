{
  "name": "@omscentral/client",
  "description": "omscentral web client",
  "license": "ISC",
  "author": "Mehmet Bajin <mehmet.bajin@gatech.edu>",
  "homepage": "https://omscentral.com",
  "repository": "github:OMSCentral/omscentral-client",
  "version": "0.0.0",
  "main": "",
  "scripts": {
    "build": "react-scripts build",
    "build:local": "env-cmd -f .env.development.local npm run build",
    "build:staging": "env-cmd -f .env.development npm run build",
    "deploy": "npm run build && firebase deploy --project omscentral-production --only hosting",
    "deploy:staging": "npm run build:staging && firebase deploy --project omscentral-staging --only hosting",
    "eject": "react-scripts eject",
    "format": "run-p format:src format:package",
    "format:package": "prettier-package-json --write ./package.json",
    "format:src": "prettier --write src/",
    "start": "react-scripts start",
    "test": "react-scripts test"
  },
  "dependencies": {
    "@apollo/react-hooks": "3.1.5",
    "@material-ui/core": "4.10.1",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.55",
    "apollo-cache-inmemory": "1.6.6",
    "apollo-client": "2.6.10",
    "apollo-link": "1.2.14",
    "apollo-link-error": "1.1.13",
    "apollo-link-http": "1.5.17",
    "clsx": "1.1.1",
    "firebase": "7.15.0",
    "graphql": "14.x",
    "graphql-tag": "2.10.3",
    "query-string": "6.12.1",
    "react": "16.13.1",
    "react-copy-to-clipboard": "5.0.2",
    "react-dom": "16.13.1",
    "react-helmet": "6.0.0",
    "react-hook-form": "5.7.2",
    "react-markdown": "4.3.1",
    "react-router-dom": "5.2.0"
  },
  "devDependencies": {
    "@types/firebase": "3.2.1",
    "@types/query-string": "6.3.0",
    "@types/react": "16.9.35",
    "@types/react-copy-to-clipboard": "4.3.0",
    "@types/react-dom": "16.9.8",
    "@types/react-helmet": "6.0.0",
    "@types/react-router-dom": "5.1.5",
    "env-cmd": "10.1.0",
    "eslint-plugin-flowtype": "4.x",
    "fibers": "5.0.0",
    "firebase-tools": "8.4.1",
    "node-sass": "4.14.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "prettier-package-json": "2.1.3",
    "react-scripts": "3.4.1",
    "sass": "1.26.8",
    "typescript": "3.9.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
